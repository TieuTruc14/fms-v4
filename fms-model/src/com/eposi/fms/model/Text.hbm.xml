<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.eposi.fms.model.Text" table="tbl_text">
		<id name="id" type="string" column="id" length="128">
			<generator class="assigned" />
		</id>
		<property name="nameE"    type="java.lang.String"  column="nameE"/>
        <property name="nameVN"   type="java.lang.String"  column="nameVN"/>
        <property name="nameTaxi" type="java.lang.String"  column="nameTaxi"/>
        <property name="nameBike" type="java.lang.String"  column="nameBike"/>
        <property name="nameShip" type="java.lang.String"  column="nameShip"/>
        <property name="nameFerry" type="java.lang.String" column="nameFerry"/>
	</class>
</hibernate-mapping>
