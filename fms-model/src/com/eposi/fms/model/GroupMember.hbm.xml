<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.eposi.fms.model.GroupMember" table="tbl_group_member">
        <composite-id>
            <key-property name="username"   column="username"  length="25" />
            <key-property name="groupId"    column="groupId" length="25" />
        </composite-id>
    </class>
</hibernate-mapping>
