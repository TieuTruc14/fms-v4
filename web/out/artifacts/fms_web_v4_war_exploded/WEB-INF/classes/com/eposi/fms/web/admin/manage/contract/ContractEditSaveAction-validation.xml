<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
        "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">

<validators>
    <field name="item.vehicle.id">
        <field-validator type="requiredstring">
            <message>Chưa nhập biển số xe</message>
        </field-validator>
    </field>
    <field name="strDateStart">
        <field-validator type="requiredstring">
            <message>Chưa nhập ngày hiệu lực</message>
        </field-validator>
    </field>
    <field name="strDateEnd">
        <field-validator type="requiredstring">
            <message>Chưa nhập ngày hết hạn</message>
        </field-validator>
    </field>

</validators>
